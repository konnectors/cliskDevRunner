{
  "name": "clisk-dev-runner",
  "version": "1.0.0",
  "description": "Projet simple pour tester les connecteurs clisk avec Playwright dans un environnement simulant React Native webview",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "start:extreme": "node src/index.js --log-level extreme",
    "start:full": "node src/index.js --log-level full",
    "start:normal": "node src/index.js --log-level normal",
    "start:quiet": "node src/index.js --log-level quiet",
    "server": "node server/app.js",
    "test": "node --test test/*.test.js",
    "test:extreme": "LOG_LEVEL=extreme node --test test/*.test.js",
    "test:full": "LOG_LEVEL=full node --test test/*.test.js",
    "test:normal": "LOG_LEVEL=normal node --test test/*.test.js",
    "test:watch": "node --test --watch test/*.test.js",
    "test:verbose": "node --test --test-reporter=spec test/*.test.js",
    "format": "prettier --write src test",
    "format:check": "prettier --check src test"
  },
  "keywords": [
    "playwright",
    "connector",
    "react-native",
    "webview",
    "clisk"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "conf": "^14.0.0",
    "debug": "^4.4.1",
    "express": "^4.19.2",
    "express-session": "^1.17.3",
    "minimist": "^1.2.8",
    "playwright": "^1.40.0",
    "post-me": "^0.4.5"
  },
  "devDependencies": {
    "prettier": "^3.6.2"
  }
}
