{
  "name": "handshake-tester",
  "version": "1.0.0",
  "description": "Projet simple pour tester les connecteurs avec playwright et post-me",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "test": "node --test test/*.test.js",
    "test:watch": "node --test --watch test/*.test.js",
    "test:verbose": "node --test --test-reporter verbose test/*.test.js"
  },
  "keywords": [
    "playwright",
    "post-me",
    "connector",
    "handshake"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "debug": "^4.4.1",
    "playwright": "^1.40.0",
    "post-me": "^0.4.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0"
  }
}
